{
  "name": "docker-with-node",
  "version": "1.0.0",
  "description": "Um repositório onde armazeno o que aprendi sobre Docker utilizando ele em aplicações Node",
  "main": "src/app.ts",
  "scripts": {
    "dev": "ts-node-dev src/app.ts --transpile-only --ignore-watch node_modules --respawn",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier ./.prettierrc --write ./src/**/*.ts{,*.js}"
  },
  "keywords": [
    "node",
    "docker",
    "nginx",
    "mongodb",
    "express"
  ],
  "repository": {
    "url": "https://github.com/Drack112/Docker-With-Node",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/Drack112/Docker-With-Node/issues"
  },
  "author": "Drack112",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-redis-cache": "^1.1.3",
    "express-winston": "^4.2.0",
    "mongoose": "^6.2.7",
    "pino": "^7.9.1",
    "pino-pretty": "^7.5.4",
    "redis": "^4.0.4",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-redis-cache": "^1.1.2",
    "@types/mongoose": "5.11.97",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
}
